@model IEnumerable<GermanProject.Models.Chapter>
@{
    ViewData["Title"] = "Home Page";
}
<div class="container col-md-6 col-md-offset-3">
    <h2 class="text-center">Kapitel</h2>
    <div class="btn-group-vertical btn-group-lg">
        @foreach (var chapter in Model)
        {
            <button type="button" class="btn btn-primary">@chapter.ChapterId: @chapter.ChapterName</button>
        }
    </div>
</div>